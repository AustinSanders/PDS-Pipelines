{
    "inst": "mo_themis_vis_edr",
    "src": "/pds_san/PDS_Archive/Mars_Odyssey/",
    "upc": {
        "recipe": {
            "thmproc": {
                "from_": "{{inputfile}}",
                "to": "{{no_extension_inputfile}}.cub",
                "mapping": "no",
                "remove": "no"
            },
            "handmos": {
                "from_": "{{no_extension_inputfile}}.raw.even.cub",
                "mosaic": "{{no_extension_inputfile}}.raw.odd.cub",
                "priority": "beneath"
            },
            "footprintinit": {
                "from_": "{{no_extension_inputfile}}.raw.odd.cub",
                "increaseprecision": "yes",
                "inctype": "vertices",
                "numvertices": "20"
            },
            "caminfo": {
                "from_": "{{no_extension_inputfile}}.raw.odd.cub",
                "to": "{{cam_info_file}}",
                "geometry": "yes",
                "isislabel": "yes",
                "originallabel": "no",
                "statistics": "yes",
                "camstats": "yes",
                "linc": "100",
                "sinc": "100",
                "polygon": "yes",
                "inctype": "vertices",
                "numvertices": "40",
                "maxemission": "180.0",
                "maxincidence": "180.0",
                "spice": "no"
            }
        }
    },
    "pow": {
        "recipe": {
            "thmproc": {
                "from_": "value",
                "to": "value",
                "remove": "no"
            },
            "handmos": {
                "from_": "value",
                "mosaic": "value",
                "priority": "beneath"
            }
        }
    },
    "reduced": {
        "recipe": {
            "thmproc": {
                "from_": "value",
                "to": "value",
                "remove": "no"
            },
            "handmos": {
                "from_": "value",
                "mosaic": "value",
                "priority": "beneath"
            },
            "reduce": {
                "from_": "value",
                "to": "value",
                "algorithm": "average",
                "mode": "scale",
                "sscale": "value",
                "lscale": "value",
                "validper": "1",
                "vper_replace": "nearest"
            },
            "isis2std": {
                "from_": "value",
                "to": "value",
                "format": "jpeg",
                "quality": "60",
                "stretch": "linear"
            }
        },
	"browse": {
	    "minlines": "200",
	    "minsamples": "200",
	    "maxlines": "900",
	    "maxsamples": "900"
	},
	"thumbnail": {
	    "minlines": "50",
	    "minsamples": "50",
	    "maxlines": "300",
	    "maxsamples": "300"
	}
    },
    "projected": {
        "recipe": {
            "thmproc": {
                "from_": "value",
                "to": "value",
                "remove": "no"
            },
            "handmos": {
                "from_": "value",
                "mosaic": "value",
                "priority": "beneath"
            },
            "isis2std": {
                "from_": "value",
                "to": "value",
                "format": "jpeg",
                "quality": "60",
                "stretch": "linear"
            }
        }
    }
}
