{
    "inst": "lro_lroc_edr_nac",
    "src": "/pds_san/PDS_Archive/Lunar_Reconnaissance_Orbiter/LROC/EDR/",
    "upc": {
        "recipe": {
            "isis.lronac2isis": {
                "from_": "{{inputfile}}",
                "to": "{{no_extension_inputfile}}.cub"
            },
            "isis.spiceinit": {
                "from_": "{{no_extension_inputfile}}.cub",
                "cknadir": "no",
                "cksmithed": "yes"
            },
            "isis.footprintinit": {
                "from_": "{{no_extension_inputfile}}.cub",
                "increaseprecision": "yes",
                "inctype": "vertices",
                "numvertices": "40"
            },
            "isis.caminfo": {
                "from_": "{{no_extension_inputfile}}.cub",
                "to": "{{cam_info_file}}",
                "geometry": "yes",
                "isislabel": "yes",
                "originallabel": "yes",
                "statistics": "yes",
                "camstats": "yes",
                "linc": "100",
                "sinc": "100",
                "uselabel": "yes",
                "spice": "no"
            }
        }
    },
    "pow": {
        "recipe": {
            "isis.lronac2isis": {
                "from_": "{{inputfile}}",
                "to": "{{no_extension_inputfile}}.cub"
            },
            "isis.spiceinit": {
                "from_": "{{no_extension_inputfile}}.cub",
                "cknadir": "no",
                "cksmithed": "yes"
            },
            "isis.lronaccal": {
                "from_": "{{no_extension_inputfile}}.cub",
                "to": "{{no_extension_inputfile}}.cal.cub"
            },
            "isis.lronacecho": {
                "from_": "{{no_extension_inputfile}}.cal.cub",
                "to": "{{no_extension_inputfile}}.echo.cub"
            },
            "isis.cam2map": {
                "from": "{{no_extension_inputfile}}.echo.cub",
                "to": "{{no_extension_inputfile}}.proj.cub",
                "map": "value",
                "matchmap": "no",
                "pixres": "value",
                "defaultrange": "value"
            }
        }
    },
    "reduced": {
        "recipe": {
            "isis.lronac2isis": {
                "from_": "{{inputfile}}",
                "to": "{{no_extension_inputfile}}.cub"
            },
            "isis.spiceinit": {
                "from_": "{{no_extension_inputfile}}.cub",
                "cknadir": "no",
                "cksmithed": "yes"
            },
            "isis.lronaccal": {
                "from_": "{{no_extension_inputfile}}.cub",
                "to": "{{no_extension_inputfile}}.cal.cub"
            },
            "gdal_translate": {
                "outputType": "Byte",
                "format": "JPEG",
                "scaleParams": [[]],
                "width": "0",
                "height": "300",
                "src": "{{no_extension_inputfile}}.cal.cub",
                "dest": "{{derived_product}}.thumbnail.jpg"
            },
            "gdal_translate": {
                "outputType": "Byte",
                "format": "JPEG",
                "scaleParams": [[]],
                "width": "0",
                "height": "900",
                "src": "{{no_extension_inputfile}}.cal.cub",
                "dest": "{{derived_product}}.browse.jpg"

            }
        }
    },
    "projected": {
        "recipe": {
            "lronac2isis": {
                "from_": "value",
                "to": "value"
            },
            "spiceinit": {
                "from_": "value",
                "cknadir": "no",
                "cksmithed": "yes"
            },
            "lronaccal": {
                "from_": "value",
                "to": "value"
            },
            "isis2std": {
                "from_": "value",
                "to": "value",
                "format": "jpeg",
                "quality": "60",
                "stretch": "linear"
            }
        }
    }
}
